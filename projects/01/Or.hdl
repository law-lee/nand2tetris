/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    // x Or y = Not(Not(x) And Not(y))
    Not(in=a,out=nota);
    Not(in=b,out=notb);
    And(a=nota,b=notb,out=o1);
    Not(in=o1,out=out);
}
